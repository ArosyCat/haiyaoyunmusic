.personalCollectedItem {
    width: 99%;
    height: 200rpx;
    line-height: 200rpx;
    margin: 10rpx 15rpx;
}

.personalCollectedItem .imageContainer {
    position: relative;
    top: 5%;
    width: 180rpx;
    height: 180rpx;
    line-height: 180rpx;
}

.personalCollectedItem .itemInfo {
    position: relative;
    top: 10%;
    margin-left: 10rpx;
    display: flex;
    flex-direction: column;
    height: 80rpx;
    line-height: 80rpx;
    width: 450rpx;
}

.personalCollectedItem .imageContainer image {
    width: 180rpx;
    height: 180rpx;
    line-height: 180rpx;
    border-radius: 50%;
}

.personalCollectedItem .itemInfo text {
    font-size: 30rpx;

}

.showMoreContainer {
    height: 40rpx;
    line-height: 40rpx;
    position: relative;
    top: 60%;
    left: 20rpx;
}

.showMoreContainer image {
    width: 40rpx;
    height: 40rpx;
}

.helpContainer {

}

.helpContainer .content{
    position: relative;
    padding: 0 25rpx;
    font-size: 30rpx;
}

.content {
    overflow: hidden;
    text-overflow: ellipsis;
    text-overflow: -o-ellipsis-lastline;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    padding: 0 25rpx;
    font-size: 30rpx;
    color: #888888;
}
.content.on {
    display: block;
    text-overflow: clip;
    overflow: visible;
}

.scrollItem {
    position: relative;
    display: flex;
    margin-bottom: 20rpx;
    border-radius: 20rpx;
}
.scrollItem image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 8rpx;
}

.musicInfo {
    display: flex;
    flex-direction: column;
    margin-left: 20rpx;
}

.musicInfo text{
    height: 40rpx;
    line-height: 40rpx;
    font-size: 24rpx;
    max-width: 500rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.scrollItem .iconfont {
    position: absolute;
    right: 0;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: right;
}
